@model Dictionary<int, Contact>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>Details</h1>
<a class="bt btn-priamry"></a></a>
<table class="table">
    <tr>
        <th>
            Imie
        </th>
        <th>
            Email
        </th>
        <th>
            Data Urodzenia
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Value.Name
            </td>
            <td>
                @item.Value.Email
            </td>
            <td>
                @item.Value.Birth
            </td>
        </tr>
    }
</table>
<a class="btn btn-primary" asp-action="Index">Powrót</a>